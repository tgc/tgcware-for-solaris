[gawk]
pkgname="${pkgprefix}${topdir}"
name="$topdir - GNU awk"
pkgcat="system"
pkgvendor="http://www.gnu.org/software/gawk/"
pkgdesc="A special-purpose programming language to do quick and easy text pattern matching and reformatting jobs"
pkgver="$pkgver"
files(-,root,bin)
default_docs
$_bindir
$_gnudir
$_infodir
$_mandir
share/locale
share/awk
libexec/awk

