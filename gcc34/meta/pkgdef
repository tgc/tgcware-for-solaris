dir $iprefix/man/man7
dir $iprefix/info
dir $iprefix/share
dir $iprefix/libexec/gcc/sparc-sun-solaris*/$version
dir $iprefix/libexec/gcc/sparc-sun-solaris*
dir $iprefix/libexec/gcc
dir $iprefix/libexec
$iprefix/bin/*gcc*
$iprefix/bin/gcov
$iprefix/bin/cpp
