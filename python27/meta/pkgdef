[python27]
pkgname="${pkgprefix}py27"
name="python27 - Programming language"
pkgcat="application"
pkgvendor="http://python.org"
pkgdesc="An interpreted, interactive, object-oriented programming language"
pkgver="$pkgver"
files(-,root,bin)
default_docs
$_bindir/pydoc${pydotver}
$_bindir/python${pydotver}
$_mandir/man1/python${pydotver}.1

